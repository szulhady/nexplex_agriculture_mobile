    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/Nex-plexIcon2.png",
        "backgroundColor": "#000000"
      },
      "package": "com.qwazx.nexplex"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
