{
  "expo": {
    "name": "Nexplex",
    "slug": "Nexplex",
    "version": "1.0.5",
    "orientation": "portrait",
    "icon": "./assets/iconNexplex2.png",
    "splash": {
      "image": "./assets/splashNexplex2.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "updates": {
